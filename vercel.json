{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/create",
      "dest": "/create.html"
    },
    {
      "src": "/",
      "dest": "/create.html"
    }
  ]
}
