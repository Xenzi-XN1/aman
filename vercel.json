{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/create",
      "dest": "/create.html"
    },
    {
      "src": "/",
      "dest": "/create.html"
    }
  ]
}
